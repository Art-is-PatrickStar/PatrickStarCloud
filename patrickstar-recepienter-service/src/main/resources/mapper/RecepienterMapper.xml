<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wsw.summercloud.mapper.RecepienterMapper">
  <resultMap id="BaseResultMap" type="com.wsw.summercloud.domain.Recepienter">
    <id column="id" jdbcType="BIGINT" property="id" />
    <result column="task_id" jdbcType="BIGINT" property="taskId"/>
    <result column="task_name" jdbcType="VARCHAR" property="taskName"/>
    <result column="name" jdbcType="VARCHAR" property="name" />
    <result column="remark" jdbcType="VARCHAR" property="remark" />
  </resultMap>

  <insert id="insert" parameterType="Recepienter">
    insert into recepienter (task_id, task_name, name, remark) values (#{taskId}, #{taskName}, #{name}, #{remark})
  </insert>

</mapper>